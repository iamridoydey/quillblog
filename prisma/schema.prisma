generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// User schema
model User {
  id Int @id @default(autoincrement())
  firstName String 
  lastName String?
  username String @unique
  email String @unique
  password String 
  profilePic String?
  title String?
  bio String?
  createdAt DateTime
}
